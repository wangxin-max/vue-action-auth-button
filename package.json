{
  "name": "vue-action-auth-button",
  "version": "1.0.0",
  "description": "一个简洁强大的 Vue 权限按钮组件，支持详情、编辑、新增、删除等操作的权限控制和二次确认",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:lib && npm run build:types",
    "build:lib": "rollup -c",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "rollup -c -w",
    "lint": "eslint src --ext .vue,.js,.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vue",
    "button",
    "auth",
    "permission",
    "authority",
    "component",
    "ui",
    "element-ui",
    "ant-design-vue",
    "权限控制",
    "按钮组件"
  ],
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/vue-action-auth-button.git"
  },
  "homepage": "https://github.com/yourusername/vue-action-auth-button#readme",
  "bugs": {
    "url": "https://github.com/yourusername/vue-action-auth-button/issues"
  },
  "peerDependencies": {
    "vue": "^2.6.0 || ^3.0.0",
    "ant-design-vue": "^1.7.0 || ^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/node": "^16.11.0",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^8.0.0",
    "rollup": "^2.60.0",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^4.5.0",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.29.0"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  }
}