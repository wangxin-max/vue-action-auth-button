{
  "name": "vue-action-auth-button",
  "version": "1.0.5",
  "description": "一个简洁强大的 Vue 权限按钮组件，支持详情、编辑、新增、删除等操作的权限控制和二次确认",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:lib && npm run build:types",
    "build:lib": "rollup -c",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "rollup -c -w",
    "serve": "http-server . -p 3000 -c-1",
    "demo": "npm run build && http-server . -p 3000 -c-1",
    "start": "cross-env NODE_ENV=development webpack serve --config webpack.demo.config.js",
    "lint": "eslint src --ext .vue,.js,.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vue",
    "button",
    "auth",
    "permission",
    "authority",
    "component",
    "ui",
    "element-ui",
    "ant-design-vue",
    "权限控制",
    "按钮组件"
  ],
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/vue-action-auth-button.git"
  },
  "homepage": "https://github.com/yourusername/vue-action-auth-button#readme",
  "bugs": {
    "url": "https://github.com/yourusername/vue-action-auth-button/issues"
  },
  "peerDependencies": {
    "ant-design-vue": "^1.7.0",
    "vue": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/node": "^16.11.0",
    "babel-loader": "^8.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.0",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^8.0.0",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.1",
    "less": "^4.4.2",
    "postcss": "^8.5.6",
    "rollup": "^2.60.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "typescript": "^4.5.0",
    "vue": "^2.6.14",
    "vue-loader": "^15.10.0",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.29.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "less-loader": "^12.3.0"
  }
}
